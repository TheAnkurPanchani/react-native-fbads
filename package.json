{
  "name": "@theankur/react-native-fbads",
  "version": "1.0.0",
  "description": "Facebook Ads for React Native",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheAnkurPanchani/react-native-fbads"
  },
  "homepage": "https://github.com/TheAnkurPanchani/react-native-fbads",
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "copyFlow": true
        }
      ],
      "module",
      "typescript"
    ]
  },
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.ts",
  "types": "lib/typescript/src/index.d.ts",
  "files": [
    "android/src/main/AndroidManifest.xml",
    "android/src/main/java/",
    "android/build.gradle",
    "ios",
    "lib",
    "src",
    "LICENSE",
    "README.md",
    "react-native-fbads.podspec"
  ],
  "directories": {
    "example": "example"
  },
  "scripts": {
    "tsc": "tsc --project .",
    "lint": "tslint --project .",
    "prepare": "bob build"
  },
  "author": "Ankur Panchani <ankur.panchani@yahoo.com>",
  "license": "MIT",
  "peerDependencies": {
    "react-native": ">=0.59.0"
  },
  "dependencies": {
    "fbemitter": "^2.1.1"
  },
  "devDependencies": {
    "@react-native-community/bob": "^0.15.1",
    "@types/fbemitter": "^2.0.32",
    "@types/react-native": "^0.57.4",
    "react-native": "^0.62.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.2"
  },
  "keywords": [
    "react-native",
    "facebook",
    "Audience Network",
    "Native ads"
  ],
  "publishConfig": {
    "access": "public"
  }
}
